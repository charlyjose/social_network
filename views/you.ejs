													iHTML += "<div class='form-group col-md-2'>";
													iHTML += "<label for='inputAddress'>Rating</label>";
													iHTML += "<input type='text' class='form-control' id='rating" + icounter + "' name='rating" + icounter + "' placeholder='Rating'>";
													iHTML += "</div>";
													iHTML += "</div>";
													iHTML += "<hr />";

													icounter++;
													iselect = icounter - 1;

													$(".interestList").append(iHTML);
												}
												else {
													alert('Type your Interest to add more');
												}
											});
										});

									</script>

								</div>
							</div>
