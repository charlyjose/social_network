 -->







					<script>

						document.addEventListener("DOMContentLoaded", function (event) {
							// Select2
							$('.select2').each(function () {
								$(this)
									.wrap('<div class="position-relative"></div>')
									.select2({
										placeholder: 'Select State',
										dropdownParent: $(this).parent()
									});
							}); ////;

							/*
							
														var i = 1;
														for (i = 1; i <= counter; ++i) {
