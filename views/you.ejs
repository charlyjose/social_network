									<script type="text/javascript">
										var counter = 2;
										var limit = 200;
										var select;
										function addSkill(divName) {
											if (counter == limit) {
												alert("You have reached the limit of adding " + counter + " inputs");
											}
											else {
												select = counter - 1;
												if (document.getElementById('skill' + select).value != '') {
													var newdiv = document.createElement('div');
													newdiv.innerHTML = "<br><input type='text' id='skill" + counter + "' class='form-control' value='' name='skill" + counter + "'>";
													document.getElementById(divName).appendChild(newdiv);

													// alert(document.getElementById('skill' + select).value);
													counter++;
												}
												else {
													alert('Type your skills to add more');
												}
											}
										}
