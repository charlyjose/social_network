									<script type="text/javascript">
										var skcounter = 2;
										var sklimit = 200;
										var skselect;
										function addSkill(divName) {
											if (skcounter == sklimit) {
												alert("You have reached the limit of adding " + skcounter + " inputs");
											}
											else {
												skselect = skcounter - 1;
												if (document.getElementById('skill' + skselect).value != '') {
